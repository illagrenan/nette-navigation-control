<ul class="breadcrumb">
	{foreach $items as $item}
		{if $iterator->isLast()}
			<li class="active">{$item->label}</li>
		{else}
			<li>
                            <a href="{$item->url}">{$item->label}</a>
                            <span class="divider">/</span>
                        </li>
		{/if}
	{/foreach}
</ul>